{
    "name": "Vditor",
    "version": "v3",
    "default_context": "通用",
    "os": ["windows", "mac", "linux"],
    "contexts": {
        "代码块": {
            "输入框和代码块之间切换": ["Alt + Enter", "Option + Enter"],
            "隐藏编辑界面": ["Esc", "Esc"],
            "选中所有代码": ["Ctrl + A", "Command + A"]
        },
        "列表 ": {
            "缩进": ["Tab or Ctrl + Shift + I", "Tab or Command + Shift + I"],
            "反向缩进": ["Shift + Tab or Ctrl + Shift + O or Enter", "Shift + Tab or Command + Shift + O or Enter"],
            "完成和待办之间切换": ["Ctrl + Shift + J", "Cmd + Shift + J"]
        },
        "引用": {
            "在顶层引用前插入空块": ["Ctrl + Alt + Enter", "Command + Option + Enter"],
            "在顶层引用后插入空块": ["Alt + Enter", "Option + Enter"],
            "插入块元素": ["Ctrl + Shift + ; or Shift + .", "Command + Shift + ; or Shift + ."],
            "引用和块元素之间切换": ["Ctrl + ;", "Command + ;"]
        },
        "标题": {
            "标题变大": ["Ctrl + Shift + EQUAL or Ctrl + NUMPAD_PLUS", "Command + Shift + EQUAL or Cmd + NUMPAD_PLUS"],
            "变小": ["Ctrl + MINUS or Ctrl + NUMPAD_MINUS", "Command + MINUS or Cmd + NUMPAD_MINUS"],
            "H1": ["Ctrl + Alt + ONE", "Command + Alt + ONE"],
            "H2": ["Ctrl + Alt + TWO", "Command + Alt + TWO"],
            "H3": ["Ctrl + Alt + THREE", "Command + Alt + THREE"],
            "H4": ["Ctrl + Alt + FOUR", "Command + Alt + FOUR"],
            "H5": ["Ctrl + Alt + FIVE", "Command + Alt + FIVE"],
            "H6": ["Ctrl + Alt + SIX", "Command + Alt + SIX"],
            "弹出菜单": ["Ctrl + H", "Command + H"]
        },
        "表格": {
            "当前行下插入新行": ["Ctrl + EQUAL or Ctrl + NUMPAD_PLUS", "Cmd + EQUAL or Cmd + NUMPAD_PLUS"],
            "删除行": ["Ctrl + MINUS or Ctrl + NUMPAD_MINUS", "Cmd + MINUS Cmd + NUMPAD_MINUS"],
            "当前列后插入新列": ["Ctrl + Shift + EQUAL or Ctrl + Shift + NUMPAD_PLUS", "Cmd + Shift + EQUAL or Cmd + Shift + NUMPAD_PLUS"],
            "删除列": ["Ctrl + Shift + MINUS or Ctrl + Shift + NUMPAD_MINUS", "Cmd + Shift + MINUS or Cmd + Shift + NUMPAD_MINUS"],
            "左对齐": ["Ctrl + Shift + L", "Cmd + Shift + L"],
            "中对齐": ["Ctrl + Shift + C", "Cmd + Shift + C"],
            "右对齐": ["Ctrl + Shift + R", "Cmd + Shift + R"],
            "光标移动到输入框中": ["Alt + Enter", "Option + Enter"],
            "输入框之间切换": ["Tab", "Tab"],
            "光标移动到上一个元素": ["Shift + Tab or Backspace", "Shift + Tab or Backspace"],
            "将光标移动到下一个元素": ["Tab", "Tab"]
        },
        "通用": {
            "表情": ["Shift + ;", "Command + E"],
            "标题": ["Ctrl + H", "Command + H"],
            "粗体": ["Ctrl + B", "Command + B"],
            "斜体": ["Ctrl + I", "Command + I"],
            "删除线": ["Ctrl + S", "Command + S"],
            "链接": ["Ctrl + K", "Command + K"],
            "无序列表": ["Ctrl + L", "Command + L"],
            "有序列表": ["Ctrl + O", "Command + O"],
            "引用": ["Ctrl + J", "Command + J"],
            "分割线": ["Ctrl + Shift + H", "Command + Shift + H"],
            "代码块": ["Ctrl + U", "Command + U"],
            "代码": ["Ctrl + G", "Command + G"],
            "元素前 插入空块": ["Ctrl + Shift + B", "Command + Shift + B"],
            "元素后 插入空块": ["Ctrl + Shift + E", "Command + Shift + E"],
            "表格": ["Ctrl + M", "Command + M"],
            "撤销": ["Ctrl + Z", "Command + Z"],
            "重做": ["Ctrl + Y", "Command + Y"],
            "隐藏编辑器": ["Ctrl + P", "Command + P"],
            "格式化": ["Ctrl + Shift + F", "Command + Shift + F"],
            "全屏": ["Ctrl + '", "Command + '"],
            "复制当前行/已选内容": ["Ctrl + Shift + D", "Command + Shift + D"],
            "删除当前行": ["Ctrl + Shift + Backspace", "Command + Shift + Backspace"],
            "向上移动块元素": ["Ctrl + Shift + U", "Command + Shift + U"],
            "向下移动块元素": ["Ctrl + Shift + D", "Command + Shift + D"],
            "移除当前元素": ["Ctrl + Shift + X", "Command + Shift + X"],
            "At 用户": ["Shift + TWO", "Shift + TWO"],
            "错误输入": ["Backspace", "Backspace"],
            "标题变大": ["Ctrl + Shift + EQUAL or Ctrl + NUMPAD_PLUS", "Command + Shift + EQUAL or Cmd + NUMPAD_PLUS"],
            "标题变小": ["Ctrl + MINUS or Ctrl + NUMPAD_MINUS", "Command + MINUS or Cmd + NUMPAD_MINUS"],
            "H1标题": ["Ctrl + Alt + ONE", "Command + Alt + ONE"],
            "H2标题": ["Ctrl + Alt + TWO", "Command + Alt + TWO"],
            "H3标题": ["Ctrl + Alt + THREE", "Command + Alt + THREE"],
            "H4标题": ["Ctrl + Alt + FOUR", "Command + Alt + FOUR"],
            "H5标题": ["Ctrl + Alt + FIVE", "Command + Alt + FIVE"],
            "H6标题": ["Ctrl + Alt + SIX", "Command + Alt + SIX"]
        },
        "链接": {
            "输入框/元素之间切换": ["Alt + Enter", "Option + Enter"],
            "输入框之间切换": ["Tab", "Tab"]
        }
    }
}
