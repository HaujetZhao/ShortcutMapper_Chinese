{
    "name" : "Vditor",
    "version" : "v3",
    "os" : "mac",
    "mods_used" : ["ALT", "COMMAND", "SHIFT"],
    "default_context" : "通用",
    "contexts" : {
        "标题" : {
            "EQUAL" : [
                {"name":"变大", "mods":["COMMAND", "SHIFT"]}
            ],
            "FIVE" : [
                {"name":"H5", "mods":["ALT", "COMMAND"]}
            ],
            "FOUR" : [
                {"name":"H4", "mods":["ALT", "COMMAND"]}
            ],
            "H" : [
                {"name":"弹出菜单", "mods":["COMMAND"]}
            ],
            "MINUS" : [
                {"name":"变小", "mods":["COMMAND"]}
            ],
            "ONE" : [
                {"name":"H1", "mods":["ALT", "COMMAND"]}
            ],
            "SIX" : [
                {"name":"H6", "mods":["ALT", "COMMAND"]}
            ],
            "THREE" : [
                {"name":"H3", "mods":["ALT", "COMMAND"]}
            ],
            "TWO" : [
                {"name":"H2", "mods":["ALT", "COMMAND"]}
            ]
        },
        "通用" : {
            "B" : [
                {"name":"元素前 插入空块", "mods":["COMMAND", "SHIFT"]},
                {"name":"粗体", "mods":["COMMAND"]}
            ],
            "BACKSPACE" : [
                {"name":"删除当前行", "mods":["COMMAND", "SHIFT"]},
                {"name":"错误输入", "mods":[]}
            ],
            "D" : [
                {"name":"复制当前行/已选内容", "mods":["COMMAND", "SHIFT"]}
            ],
            "E" : [
                {"name":"元素后 插入空块", "mods":["COMMAND", "SHIFT"]},
                {"name":"表情", "mods":["COMMAND"]}
            ],
            "F" : [
                {"name":"格式化", "mods":["COMMAND", "SHIFT"]}
            ],
            "G" : [
                {"name":"代码", "mods":["COMMAND"]}
            ],
            "H" : [
                {"name":"分割线", "mods":["COMMAND", "SHIFT"]},
                {"name":"标题", "mods":["COMMAND"]}
            ],
            "I" : [
                {"name":"斜体", "mods":["COMMAND"]}
            ],
            "J" : [
                {"name":"引用", "mods":["COMMAND"]}
            ],
            "K" : [
                {"name":"链接", "mods":["COMMAND"]}
            ],
            "L" : [
                {"name":"无序列表", "mods":["COMMAND"]}
            ],
            "M" : [
                {"name":"表格", "mods":["COMMAND"]}
            ],
            "O" : [
                {"name":"有序列表", "mods":["COMMAND"]}
            ],
            "P" : [
                {"name":"隐藏编辑器", "mods":["COMMAND"]}
            ],
            "S" : [
                {"name":"删除线", "mods":["COMMAND"]}
            ],
            "SINGLE_QUOTE" : [
                {"name":"全屏", "mods":["COMMAND"]}
            ],
            "TWO" : [
                {"name":"At 用户", "mods":["SHIFT"]}
            ],
            "U" : [
                {"name":"代码块", "mods":["COMMAND"]},
                {"name":"向上移动块元素", "mods":["COMMAND", "SHIFT"]}
            ],
            "X" : [
                {"name":"移除当前元素", "mods":["COMMAND", "SHIFT"]}
            ],
            "Y" : [
                {"name":"重做", "mods":["COMMAND"]}
            ],
            "Z" : [
                {"name":"撤销", "mods":["COMMAND"]}
            ]
        }
    }
}
